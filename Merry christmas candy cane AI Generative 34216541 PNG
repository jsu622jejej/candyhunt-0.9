      <input type="text" name="search_fullwidth" id="search_fullwidth" value="" class="ez-input__field ez-input__field--basic ez-input__field ez-input__field--text ez-search-form__input ez-input__field--text" placeholder="Search PNGs..." autocomplete="off" data-search-form-target="input" data-action="blur->search-form#resetAutocomplete keyup->search-form#handleInputKeyUp keydown->search-form#handleInputKeyDown focus->form-analytics#sendFormFieldFocusEvent" data-content-type="png" data-search-route="search" data-autocomplete-source="https://autocomplete.vecteezy.com/autocomplete/vecteezy/en" data-autocomplete-content-type="png" data-search-by-image-target="searchInput" data-english-label="search_fullwidth">

